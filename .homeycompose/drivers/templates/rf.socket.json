{
  "pair": [
    {
      "$template": "rf.choice",
      "id": "rf.choice",
      "options": {
        "title": "rf.pair.choice.title.switch",
        "body": "rf.pair.choice.body.switch",
        "choices": [
          {
            "name": "rf.pair.choice.buttons.copy.default",
            "view": "rf.imitate",
            "svg": "./assets/svg/remote.svg"
          },
          {
            "name": "rf.pair.choice.buttons.generate.default",
            "view": "rf.generate",
            "svg": "./assets/svg/socket.svg"
          }
        ]
      }
    },
    {
      "$template": "rf.imitate",
      "id": "rf.imitate",
      "navigation": {
        "prev": "rf.choice"
      }
    },
    {
      "$template": "rf.test_switch",
      "id": "rf.test_switch",
      "navigation": {
        "prev": "rf.imitate",
        "next": "rf.choose_slave"
      }
    },
    {
      "$template": "rf.choose_slave",
      "id": "rf.choose_slave",
      "navigation": {
        "prev": "rf.test_switch",
        "next": "rf.done"
      }
    },
    {
      "$template": "rf.program",
      "navigation": {
        "prev": "rf.choice"
      }
    },
    {
      "$template": "rf.test_switch",
      "id": "rf.test_switch.2",
      "options": {
        "body": "rf.pair.test.body.switch_generated"
      },
      "navigation": {
        "prev": "rf.program",
        "next": "rf.choose_slave.2"
      }
    },
    {
      "$template": "rf.choose_slave",
      "id": "rf.choose_slave.2",
      "navigation": {
        "prev": "rf.test_switch.2",
        "next": "rf.done"
      }
    },
    {
      "$template": "rf.done",
      "id": "rf.done"
    }
  ],
  "class": "socket",
  "capabilities": [
    "onoff"
  ]
}
